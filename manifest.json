{
  "name": "Warning Goose",
  "description": "Warning Goose - Crowdsourced Privacy Alert",
  "version": "1.1.1",
  "manifest_version": 2,
  "author": "Warning Goose",
  "homepage_url": "https://warninggoose.net",
  "offline_enabled": false,
  "short_name": "Warning Goose",
  "icons": {
      "48": "meta/img/icon-48.png",
      "96": "meta/img/icon-96.png",
      "128": "meta/img/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; default-src 'self';",
  "browser_action": {
    "default_icon": {
      "48": "meta/img/icon-48.png",
      "96": "meta/img/icon-96.png",
      "128": "meta/img/icon-128.png"
    },
    "default_popup": "popup/dist/index.html",
    "default_title": "Warning Goose"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
